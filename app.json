{
  "expo": {
    "name": "Image to Video App",
    "slug": "image-to-video-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "n",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "package": "com.nguyenhungdunggithub.imagetovideo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "react-native-video"
    ],
    "experimental": {
      "router": true
    },
    "owner": "nguyenhungdunggithub",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ccc371f2-b6b4-4b83-ac5b-4815dd0acf58"
      }
    }
  }
}
